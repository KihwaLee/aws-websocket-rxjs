{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["google", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "new-cap": "off",
        "valid-jsdoc": "off",
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ]
    }
}
